<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="GridBackground" EndPoint="1,1" StartPoint="0.25,0.25" Opacity="0.6">
        <GradientStop Color="#ace0f9" Offset="0"/>
        <GradientStop Color="#fbc8d4 " Offset="1"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="ElementBackground" Color="White" Opacity="0.6"/>
    <SolidColorBrush x:Key="NavigationViewTopPaneBackground" Color="#fbc8d4" Opacity="0.8"/>
    <SolidColorBrush x:Key="NavigationViewExpandedPaneBackground" Color="#fbc8d4"/>
    <SolidColorBrush x:Key="NavigationViewDefaultPaneBackground" Color="#fbc8d4"/>
    <SolidColorBrush x:Key="ButtonForegroundPointerOver" Color="Black"/>
    <SolidColorBrush x:Key="ButtonBackgroundPointerOver" Color="SkyBlue"/>
    <SolidColorBrush x:Key="ButtonBackground" Color="LightBlue"/>
</ResourceDictionary>
